package wipro_java_project_oops;

public class Sedan extends Car {

  
    public Sedan(String make, String model, int year) {
        super( model, year); // Calling Car's constructor
    }

   
    @Override
    public void displayCarDetails() {
        System.out.println("This is a Sedan: " + getModel() + " from year " + getYear());
    }

    public static void main(String[] args) {
        Car sedanCar = new Sedan("Honda", "City", 2022);
        sedanCar.displayCarDetails();  
    }
}
