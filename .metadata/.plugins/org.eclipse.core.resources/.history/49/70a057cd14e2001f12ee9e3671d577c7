package wipro_java_project_oops;
public class Innova extends Car {


	public void displayCarDetails() {
        System.out.println("This is a Sedan: " + getModel() +" from year " + getYear());
        		

	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        	Car sedanCar=new Sedan();
        	sedanCar.setModel("Sedan");
        	sedanCar.setYear(2022);
        	sedanCar.displayCarDetails();
        	

	}

}

