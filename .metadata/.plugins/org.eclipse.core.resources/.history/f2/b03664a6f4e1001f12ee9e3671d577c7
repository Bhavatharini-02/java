package wipro_java_project_oops;
/**NO constructor
* Getters have return value
* setters have no return value
* properties are determined using private fileds
* values are behaviors are determined through setters
* toString will convert the entire animal class with properties and behaviors
*/

public class Animal {
	private String name;
	private String species;
	private int age;
	/**
	 * @return
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name
	 */
	public void setName(String name) {
		this.name = "Fish";
	}

	/**
	 * @return
	 */
	public String getSpecies() {
		return species;
	}

	/**
	 * @param species
	 */
	public void setSpecies(String species) {
		this.species = "Aquatic species";
	}

	/**
	 * @return
	 */
	public int getAge() {
		return age;
	}

	/**
	 * @param age
	 */
	public void setAge(int age) {
		this.age = 15;
	}

	

	public String toString() {
		return "Animal{" +
        "name='" + name + '\'' +
        ", species='" + species + '\'' +
        ", age=" + age +
        '}';
	}

}
